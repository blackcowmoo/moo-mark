{
  "extends": ["../.eslintrc", "plugin:react/recommended"],
  "ignorePatterns": ["dist/", "build/"],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./"]],
        "extensions": [".ts", ".tsx", ".js", ".jsx"]
      }
    }
  },
  "rules": {
    "react/prop-types": ["off"],
    "react/react-in-jsx-scope": ["off"]
  }
}
